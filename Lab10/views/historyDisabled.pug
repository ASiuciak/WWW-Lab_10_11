html
    head
        title= title
    style
        include history.css
    body
        h1= title
        img(src=meme.url)
        table
            tr
                th= 'Prices history'
            each price in prices
                tr
                    td= price
        h1= "Change price"
        h1= "You must be logged in to change the price or see exact history."
        a(href="../..") Back to main page
        h2= "Pages visited: "+ pages